#################################################################
#    Object Properties
#################################################################

###  https://dmikhaylova.github.io/itdt_ontology/#hasFormalBuildingType
:hasFormalBuildingType rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf rico:hasOrHadCategory ;
                       owl:inverseOf :isFormalBuildingTypeOf ;
                       rdfs:domain :ArchitecturalProject ;
                       rdfs:range :FormalBuildingType ;
                       rdfs:comment "Connects an Architectural Project to its Formal Building Type."@en ;
                       rdfs:label "has formal building type"@en .


###  https://dmikhaylova.github.io/itdt_ontology/#hasFunctionalBuildingType
:hasFunctionalBuildingType rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf rico:hasOrHadCategory ;
                           owl:inverseOf :isFunctionalBuildingTypeOf ;
                           rdfs:domain :ArchitecturalProject ;
                           rdfs:range :FunctionalBuildingType ;
                           rdfs:comment "Connects an Architectural Project to its Functional Building Type."@en ;
                           rdfs:label "has functional building type"@en .


###  https://dmikhaylova.github.io/itdt_ontology/#hasOrHadProjectPhase
:hasOrHadProjectPhase rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf rico:hasOrHadPart ;
                      owl:inverseOf :isOrWasProjectPhaseOf ;
                      rdfs:domain :ArchitecturalProject ;
                      rdfs:range :ProjectPhase ;
                      rdfs:comment "Connects an Architectural Project to its Project Phase"@en ;
                      rdfs:label "has or had project phase"@en .


###  https://dmikhaylova.github.io/itdt_ontology/#hasOrHadProjectRole
:hasOrHadProjectRole rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf rico:isRelatedTo ;
                     owl:inverseOf :isOrWasProjectRoleOf ;
                     rdfs:domain :ProjectParticipation ;
                     rdfs:range :ProjectRole ;
                     rdfs:comment "Connects Project Participation with the Project Role the Agent had in the Architectural Project."@en ;
                     rdfs:label "has or had project role"@en .


###  https://dmikhaylova.github.io/itdt_ontology/#hasOrHasProjectParticipation
:hasOrHasProjectParticipation rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf rico:isEventAssociatedWith ;
                              owl:inverseOf :isOrWasProjectParticipationOf ;
                              rdfs:domain rico:Activity ;
                              rdfs:range :ProjectParticipation ;
                              rdfs:comment "Connects an Architectural Project o Project Phase to a Project Participation."@en ;
                              rdfs:label "has or had project participation"@en .


###  https://dmikhaylova.github.io/itdt_ontology/#hasProjectScale
:hasProjectScale rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf rico:hasOrHadCategory ;
                 owl:inverseOf :isProjectScaleOf ;
                 rdfs:domain :ArchitecturalProject ;
                 rdfs:range :ProjectScale ;
                 rdfs:comment "Connects an Architectural Project to its Project Scale"@en ;
                 rdfs:label "has project scale"@en .


###  https://dmikhaylova.github.io/itdt_ontology/#isCommissionedBy
:isCommissionedBy rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf rico:hasProvenance ;
                  owl:inverseOf :isCommissionerOf ;
                  rdfs:domain :ArchitecturalProject ;
                  rdfs:range rico:Agent ;
                  rdfs:comment "Connects an Architectural Project to an Agent that commissioned its realisation."@en ;
                  rdfs:label "is commissioned by"@en .


###  https://dmikhaylova.github.io/itdt_ontology/#isCommissionerOf
:isCommissionerOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf rico:isProvenanceOf ;
                  rdfs:domain rico:Agent ;
                  rdfs:range :ArchitecturalProject ;
                  rdfs:comment "Connects an Agent to an Architectural project that they commissioned."@en ;
                  rdfs:label "is commissioner of"@en .


###  https://dmikhaylova.github.io/itdt_ontology/#isFormalBuildingTypeOf
:isFormalBuildingTypeOf rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf rico:isOrWasCategoryOf ;
                        rdfs:domain :FormalBuildingType ;
                        rdfs:range :ArchitecturalProject ;
                        rdfs:comment "Connects a Formal Building Type to an Architectural Project that it categorises."@en ;
                        rdfs:label "is formal building type of"@en .


###  https://dmikhaylova.github.io/itdt_ontology/#isFunctionalBuildingTypeOf
:isFunctionalBuildingTypeOf rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf rico:isOrWasCategoryOf ;
                            rdfs:domain :FunctionalBuildingType ;
                            rdfs:range :ArchitecturalProject ;
                            rdfs:comment "Connects a Functional Building Type to an Architectural Project that it categorises."@en ;
                            rdfs:label "is functional building type of"@en .


###  https://dmikhaylova.github.io/itdt_ontology/#isOrWasDescribedBy
:isOrWasDescribedBy rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf rico:isOrWasSubjectOf ;
                    owl:inverseOf rico:describesOrDescribed ;
                    rdfs:range rico:RecordResource ;
                    rdfs:comment "inverse of describes or described"@en ;
                    rdfs:label "is or was described by"@en .


###  https://dmikhaylova.github.io/itdt_ontology/#isOrWasProjectParticipationOf
:isOrWasProjectParticipationOf rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf rico:isAssociatedWithEvent ;
                               rdfs:domain :ProjectParticipation ;
                               rdfs:range rico:Activity ;
                               rdfs:comment "Connects Project Participation to an Architectural Project or to a Project Phase in which an Agent participated with a specific Project Role"@en ;
                               rdfs:label "is or was project participation of"@en .


###  https://dmikhaylova.github.io/itdt_ontology/#isOrWasProjectPhaseOf
:isOrWasProjectPhaseOf rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf rico:isOrWasPartOf ;
                       rdfs:domain :ProjectPhase ;
                       rdfs:range :ArchitecturalProject ;
                       rdfs:comment "Connects a Project Phase to an Architectural Project of which it was part."@en ;
                       rdfs:label "is or was project phase of"@en .


###  https://dmikhaylova.github.io/itdt_ontology/#isOrWasProjectRoleOf
:isOrWasProjectRoleOf rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf rico:isRelatedTo ;
                      rdfs:domain :ProjectRole ;
                      rdfs:range :ProjectParticipation ;
                      rdfs:comment "Connects a Project Participation to a Project Role with which an Agent participated in a Architectural project connected by the same Project Participation."@en ;
                      rdfs:label "is or was project role of"@en .


###  https://dmikhaylova.github.io/itdt_ontology/#isProjectScaleOf
:isProjectScaleOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf rico:isOrWasCategoryOf ;
                  rdfs:domain :ProjectScale ;
                  rdfs:range :ArchitecturalProject ;
                  rdfs:comment "Connects a Project Scale to an Architectural Project that it categorises."@en ;
                  rdfs:label "is project scale of"@en .


###  https://www.ica.org/standards/RiC/ontology#containsOrContained
rico:containsOrContained rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf rico:hasOrHadPart ,
                                            rico:isAssociatedWithPlace ;
                         owl:inverseOf rico:isOrWasContainedBy ;
                         rdfs:domain rico:Place ;
                         rdfs:range rico:Place ;
                         rdfs:comment """Connects a Place to a region that is or was within
            it."""@en ;
                         rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                         rdfs:label "contains or contained"@en ;
                         <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
<http://www.w3.org/2004/02/skos/core#scopeNote> """Use for connecting two geographical or administrative
            regions."""@en ;
rico:RiCCMCorrespondingComponent """RiC-R007 ('contains or contained'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#describesOrDescribed
rico:describesOrDescribed rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf rico:hasOrHadSubject ;
                          rdfs:domain rico:RecordResource ;
                          rdfs:comment """Connects a Record Resource to a Thing that it
            describes."""@en ;
                          rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                          rdfs:label "describes or described"@en ;
                          <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
<http://www.w3.org/2004/02/skos/core#scopeNote> """Can be used, among other situations, for specifying that some
            finding aid (a Record that has Documentary Form Type Finding Aid) describes some Record
            Set."""@en ;
rico:RiCCMCorrespondingComponent """RiC-R021 (describes or
            described)"""@en .


###  https://www.ica.org/standards/RiC/ontology#documentedBy
rico:documentedBy rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf rico:resultsOrResultedIn ;
                  owl:inverseOf rico:documents ;
                  rdfs:domain rico:Activity ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( rico:Instantiation
                                             rico:RecordResource
                                           )
                             ] ;
                  rdfs:comment "Inverse of 'documents' object property."@en ;
                  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                  rdfs:label "documented by"@en ;
                  <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
rico:RiCCMCorrespondingComponent """RiC-R033i ('documented by'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#documents
rico:documents rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf rico:resultsOrResultedFrom ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( rico:Instantiation
                                           rico:RecordResource
                                         )
                           ] ;
               rdfs:range rico:Activity ;
               owl:propertyChainAxiom ( rico:documents
                                        :isOrWasProjectPhaseOf
                                      ) ;
               rdfs:comment """Connects a Record Resource or an Instantiation to the Activity
            that generates the Record Resource or Instantiation."""@en ;
               rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
               rdfs:label "documents"@en ;
               <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
rico:RiCCMCorrespondingComponent """RiC-R033 ('documents'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#followsInTime
rico:followsInTime rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf rico:followsOrFollowed ;
                   owl:inverseOf rico:precedesInTime ;
                   rdfs:label "follows in time"@en .


###  https://www.ica.org/standards/RiC/ontology#followsOrFollowed
rico:followsOrFollowed rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf rico:isRelatedTo ;
                       owl:inverseOf rico:precedesOrPreceded ;
                       rdfs:comment "Inverse of 'precedesOrPreceded' object property."@en ;
                       rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                       rdfs:label "follows or followed"@en ;
                       <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
rico:RiCCMCorrespondingComponent """RiC-R008i ('follows or followed'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasAuthor
rico:hasAuthor rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf rico:hasCreator ;
               owl:inverseOf rico:isAuthorOf ;
               rdfs:domain rico:Record ;
               rdfs:comment """Connects a Record to the Group, Person or Position that is
            responsible for conceiving and formulating the information contained in the
            Record."""@en ;
               rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
               rdfs:label "has author"@en ;
               <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
<http://www.w3.org/2004/02/skos/core#scopeNote> """To be used for any contribution to the content of a Record.
            Includes (of course) the Person, Group or Position in whose name or by whose command the
            content may have been formulated and first instantiated (e.g. the person who signed
            it)."""@en ;
rico:RiCCMCorrespondingComponent """RiC-R079 ('has author'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasBeginningDate
rico:hasBeginningDate rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf rico:isAssociatedWithDate ;
                      owl:inverseOf rico:isBeginningDateOf ;
                      rdfs:range rico:Date ;
                      rdfs:comment "Inverse of 'is beginning date of' object property"@en ;
                      rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                      rdfs:label "has beginning date "@en ;
                      rico:RiCCMCorrespondingComponent """RiC-R069i ('has beginning date'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasBirthDate
rico:hasBirthDate rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf rico:hasBeginningDate ;
                  owl:inverseOf rico:isBirthDateOf ;
                  rdfs:domain rico:Person ;
                  rdfs:range rico:Date ;
                  rdfs:comment "Inverse of 'is birth date of' object property"@en ;
                  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                  rdfs:label "has birth date "@en ;
                  rico:RiCCMCorrespondingComponent """RiC-R070i ('has birth date'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasCreator
rico:hasCreator rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf rico:hasProvenance ;
                owl:inverseOf rico:isCreatorOf ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( rico:Instantiation
                                            rico:RecordResource
                                          )
                            ] ;
                rdfs:range rico:Agent ;
                rdfs:comment """Connects a Record Resource or an Instantiation to an Agent that
            is either responsible for all or some of the content of the Record Resource or is a
            contributor to the genesis or production of an Instantiation."""@en ;
                rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                rdfs:label "has creator"@en ;
                <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
<http://www.w3.org/2004/02/skos/core#scopeNote> """Covers the definition of author in diplomatics, and any
            contribution to the intellectual content of a Record Resource."""@en ;
rico:RiCCMCorrespondingComponent """RiC-R027 ('has creator'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasDeathDate
rico:hasDeathDate rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf rico:hasEndDate ;
                  owl:inverseOf rico:isDeathDateOf ;
                  rdfs:domain rico:Person ;
                  rdfs:range rico:Date ;
                  rdfs:comment "Inverse of 'is death date of' object property"@en ;
                  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                  rdfs:label "has death date "@en ;
                  rico:RiCCMCorrespondingComponent """RiC-R072i ('has death date'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasDerivedInstantiation
rico:hasDerivedInstantiation rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf rico:precedesInTime ;
                             owl:inverseOf rico:isDerivedFromInstantiation ;
                             rdfs:domain rico:Instantiation ;
                             rdfs:range rico:Instantiation ;
                             rdfs:comment """Connects an Instantiation to an Instantiation that is derived
            from it."""@en ;
                             rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                             rdfs:label "has derived instantiation "@en ;
                             rico:RiCCMCorrespondingComponent """RiC-R014 ('has derived instantiation'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasDocumentaryFormType
rico:hasDocumentaryFormType rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf rico:hasOrHadCategory ;
                            owl:inverseOf rico:isDocumentaryFormTypeOf ;
                            rdfs:domain [ rdf:type owl:Class ;
                                          owl:unionOf ( rico:Record
                                                        rico:RecordPart
                                                      )
                                        ] ;
                            rdfs:range rico:DocumentaryFormType ;
                            rdfs:comment """Connects a Record or Record Part to its Documentary Form
            Type."""@en ;
                            rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                            rdfs:label "has documentary form type"@en ;
                            <http://www.w3.org/2004/02/skos/core#changeNote> [ ] .


###  https://www.ica.org/standards/RiC/ontology#hasEndDate
rico:hasEndDate rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf rico:isAssociatedWithDate ;
                owl:inverseOf rico:isEndDateOf ;
                rdfs:range rico:Date ;
                rdfs:comment "Inverse of 'is end date of' object property."@en ;
                rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                rdfs:label "has end date "@en ;
                rico:RiCCMCorrespondingComponent """RiC-R071i ('has end date'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasInstantiation
rico:hasInstantiation rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf rico:isRelatedTo ;
                      owl:inverseOf rico:isInstantiationOf ;
                      rdfs:domain rico:RecordResource ;
                      rdfs:range rico:Instantiation ;
                      rdfs:comment """Connects a Record Resource to one of its
            Instantiations."""@en ;
                      rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                      rdfs:label "has instantiation"@en ;
                      <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ,
[ ] ;
rico:RiCCMCorrespondingComponent """RiC-R025 ('has instantiation'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasOrHadAuthorityOver
rico:hasOrHadAuthorityOver rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf rico:isRelatedTo ;
                           owl:inverseOf rico:isOrWasUnderAuthorityOf ;
                           rdfs:domain rico:Agent ;
                           rdfs:comment """Connects an Agent to a Thing the Agent has or had authority
            over."""@en ;
                           rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                           rdfs:label "has or had authority over"@en ;
                           <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
rico:RiCCMCorrespondingComponent """RiC-R036 ('has or had authority over'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasOrHadCategory
rico:hasOrHadCategory rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf rico:isRelatedTo ;
                      owl:inverseOf rico:isOrWasCategoryOf ;
                      rdfs:domain rico:Thing ;
                      rdfs:range rico:Type ;
                      rdfs:comment """Connects a Thing to a Type that categorizes or categorized
            it."""@en ;
                      rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                      rdfs:label "has or had category"@en ;
                      <http://www.w3.org/2004/02/skos/core#changeNote> [ ] .


###  https://www.ica.org/standards/RiC/ontology#hasOrHadController
rico:hasOrHadController rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf rico:isOrWasUnderAuthorityOf ;
                        owl:inverseOf rico:isOrWasControllerOf ;
                        rdfs:domain rico:Agent ;
                        rdfs:range rico:Agent ;
                        rdfs:comment """Inverse of 'is or was controller of' object
            property."""@en ;
                        rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                        rdfs:label "has or had controller"@en ;
                        <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
rico:RiCCMCorrespondingComponent """RiC-R041i ('has or had controller'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasOrHadLeader
rico:hasOrHadLeader rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf rico:hasOrHadController ;
                    owl:inverseOf rico:isOrWasLeaderOf ;
                    rdfs:domain rico:Group ;
                    rdfs:range rico:Person ;
                    rdfs:comment "Inverse of 'is or was leader of' object property."@en ;
                    rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                    rdfs:label "has or had leader"@en ;
                    <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
rico:RiCCMCorrespondingComponent """RiC-R042i ('has or had leader'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasOrHadMainSubject
rico:hasOrHadMainSubject rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf rico:hasOrHadSubject ;
                         owl:inverseOf rico:isOrWasMainSubjectOf ;
                         rdfs:domain rico:RecordResource ;
                         rdfs:comment """Connects a Record Resource to a Thing that is or was its main
            subject."""@en ;
                         rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
