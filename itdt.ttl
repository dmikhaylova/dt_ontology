                                                                                      owl:onClass <https://dmikhaylova.github.io/itdt_ontology/#ProjectRole>
                                                                                    ] ,
                                                                                    [ rdf:type owl:Restriction ;
                                                                                      owl:onProperty <https://dmikhaylova.github.io/itdt_ontology/#isOrWasProjectParticipationOf> ;
                                                                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                      owl:onClass [ rdf:type owl:Class ;
