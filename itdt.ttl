###  https://www.ica.org/standards/RiC/ontology#containsOrContained
rico:containsOrContained rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf rico:hasOrHadPart ,
                                            rico:isAssociatedWithPlace ;
                         owl:inverseOf rico:isOrWasContainedBy ;
                         rdfs:domain rico:Place ;
                         rdfs:range rico:Place ;
                         rdfs:comment """Connects a Place to a region that is or was within
            it."""@en ;
                         rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                         rdfs:label "contains or contained"@en ;
                         <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
<http://www.w3.org/2004/02/skos/core#scopeNote> """Use for connecting two geographical or administrative
            regions."""@en ;
rico:RiCCMCorrespondingComponent """RiC-R007 ('contains or contained'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#describesOrDescribed
rico:describesOrDescribed rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf rico:hasOrHadSubject ;
                          rdfs:domain rico:RecordResource ;
                          rdfs:comment """Connects a Record Resource to a Thing that it
            describes."""@en ;
                          rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                          rdfs:label "describes or described"@en ;
                          <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
<http://www.w3.org/2004/02/skos/core#scopeNote> """Can be used, among other situations, for specifying that some
            finding aid (a Record that has Documentary Form Type Finding Aid) describes some Record
            Set."""@en ;
rico:RiCCMCorrespondingComponent """RiC-R021 (describes or
            described)"""@en .


###  https://www.ica.org/standards/RiC/ontology#documentedBy
rico:documentedBy rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf rico:resultsOrResultedIn ;
                  owl:inverseOf rico:documents ;
                  rdfs:domain rico:Activity ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( rico:Instantiation
                                             rico:RecordResource
                                           )
                             ] ;
                  rdfs:comment "Inverse of 'documents' object property."@en ;
                  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                  rdfs:label "documented by"@en ;
                  <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
rico:RiCCMCorrespondingComponent """RiC-R033i ('documented by'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#documents
rico:documents rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf rico:resultsOrResultedFrom ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( rico:Instantiation
                                           rico:RecordResource
                                         )
                           ] ;
               rdfs:range rico:Activity ;
               owl:propertyChainAxiom ( rico:documents
                                        <https://dmikhaylova.github.io/itdt_ontology/#isOrWasProjectPhaseOf>
                                      ) ;
               rdfs:comment """Connects a Record Resource or an Instantiation to the Activity
            that generates the Record Resource or Instantiation."""@en ;
               rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
               rdfs:label "documents"@en ;
               <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
rico:RiCCMCorrespondingComponent """RiC-R033 ('documents'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#followsInTime
rico:followsInTime rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf rico:followsOrFollowed ;
                   owl:inverseOf rico:precedesInTime ;
                   rdfs:label "follows in time"@en .


###  https://www.ica.org/standards/RiC/ontology#followsOrFollowed
rico:followsOrFollowed rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf rico:isRelatedTo ;
                       owl:inverseOf rico:precedesOrPreceded ;
                       rdfs:comment "Inverse of 'precedesOrPreceded' object property."@en ;
                       rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                       rdfs:label "follows or followed"@en ;
                       <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
rico:RiCCMCorrespondingComponent """RiC-R008i ('follows or followed'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasAuthor
rico:hasAuthor rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf rico:hasCreator ;
               owl:inverseOf rico:isAuthorOf ;
               rdfs:domain rico:Record ;
               rdfs:comment """Connects a Record to the Group, Person or Position that is
            responsible for conceiving and formulating the information contained in the
            Record."""@en ;
               rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
               rdfs:label "has author"@en ;
               <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
<http://www.w3.org/2004/02/skos/core#scopeNote> """To be used for any contribution to the content of a Record.
            Includes (of course) the Person, Group or Position in whose name or by whose command the
            content may have been formulated and first instantiated (e.g. the person who signed
            it)."""@en ;
rico:RiCCMCorrespondingComponent """RiC-R079 ('has author'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasBeginningDate
rico:hasBeginningDate rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf rico:isAssociatedWithDate ;
                      owl:inverseOf rico:isBeginningDateOf ;
                      rdfs:range rico:Date ;
                      rdfs:comment "Inverse of 'is beginning date of' object property"@en ;
                      rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                      rdfs:label "has beginning date "@en ;
                      rico:RiCCMCorrespondingComponent """RiC-R069i ('has beginning date'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasBirthDate
rico:hasBirthDate rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf rico:hasBeginningDate ;
                  owl:inverseOf rico:isBirthDateOf ;
                  rdfs:domain rico:Person ;
                  rdfs:range rico:Date ;
                  rdfs:comment "Inverse of 'is birth date of' object property"@en ;
                  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                  rdfs:label "has birth date "@en ;
                  rico:RiCCMCorrespondingComponent """RiC-R070i ('has birth date'
            relation)"""@en .


###  https://www.ica.org/standards/RiC/ontology#hasCreator
rico:hasCreator rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf rico:hasProvenance ;
                owl:inverseOf rico:isCreatorOf ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( rico:Instantiation
                                            rico:RecordResource
                                          )
                            ] ;
                rdfs:range rico:Agent ;
                rdfs:comment """Connects a Record Resource or an Instantiation to an Agent that
            is either responsible for all or some of the content of the Record Resource or is a
            contributor to the genesis or production of an Instantiation."""@en ;
                rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
                rdfs:label "has creator"@en ;
                <http://www.w3.org/2004/02/skos/core#changeNote> [ ] ;
